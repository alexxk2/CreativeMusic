<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="MainButtonsStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/main_menu_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="iconTint">@color/black</item>
        <item name="iconGravity">textStart</item>
        <item name="iconSize">@dimen/main_menu_icon_size</item>
    </style>

    <style name="YSTextStyleWhite22">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/main_menu_text_size</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="YSTextStyleBlack22">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/main_menu_text_size</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="YSTextStyleBlack16">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

</resources>
